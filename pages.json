		"navigationBarBackgroundColor": "#F8F8F8",
		"backgroundColor": "#F7F7F7"
	},
	"easycom": {
		"autoscan": true,
		"custom": {
			"xfab-(.*)": "@/components/xfab-$1/xui-$1.vue",
			"tui-(.*)": "@/components/thorui/tui-$1/tui-$1.vue"
		}
